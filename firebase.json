{
  "hosting": {
    "public": "docs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [

      {
        "source": "/*",
        "headers": [
          {
            "key": "Link",
            "value": "</dist/app.js>;rel=preload;as=script"
          }
        ]
      },
      {
        "source": "favicon.ico",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=604800,s-maxage=604800"
          }
        ]
      },
      {
        "source": "/dist/app.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=604800,s-maxage=604800"
          }
        ]
      }
    ]
  }
}
